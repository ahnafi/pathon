{
  "name": "pathon",
  "description": "Download on your path",
  "version": "0.3",
  "manifest_version": 3,
  "permissions": ["downloads"],
  "browser_specific_settings": {
    "gecko": {
      "id": "pathon@sulthon.extension"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}